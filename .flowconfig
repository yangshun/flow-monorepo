[ignore]
.*/dist/.*
.*/node_modules/.*

[include]
./packages/

[strict]
deprecated-type
sketchy-null
unclear-type
unsafe-getters-setters

[lints]
all=warn

[options]
exact_by_default=true

module.name_mapper='@yangshun/add' -> '<PROJECT_ROOT>/packages/add/src/index.js'
module.name_mapper='@yangshun/subtract' -> '<PROJECT_ROOT>/packages/subtract/src/index.js'
