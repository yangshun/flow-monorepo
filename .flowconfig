[ignore]
.*/dist/.*
.*/node_modules/.*

[include]
./packages/

[strict]
deprecated-type
sketchy-null
unclear-type
unsafe-getters-setters

[lints]
all=warn

[options]
emoji=true
exact_by_default=true
experimental.const_params=true
module.use_strict=true
munge_underscores=true
suppress_type=$FlowFixMe
suppress_type=$FlowTODO
module.name_mapper='^@yangshun/\(.+\)$' -> '<PROJECT_ROOT>/packages/\1/src/index.js'
